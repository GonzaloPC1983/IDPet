        </TableRow>

        <TableRow
            android:layout_height="wrap_content"
            android:background="#EFC7F6">

            <TextView
                android:id="@+id/ciudadNacimiento"
                android:layout_width="183dp"
                android:layout_height="30dp"
                android:text="CIUDAD DE NACIMIENTO"
                android:gravity="center"
