            <TextView
                android:id="@+id/tvclase"
                android:layout_width="41dp"
                android:layout_height="30dp"
                android:gravity="center"
                android:text=""
                android:textAlignment="center" />
        </TableRow>

        <TableRow
            android:layout_height="wrap_content"
            android:background="#EFC7F6">

