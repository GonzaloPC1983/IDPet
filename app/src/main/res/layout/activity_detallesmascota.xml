        <TableRow
            android:layout_height="wrap_content"
            android:background="#89C6F6">

            <TextView
                android:id="@+id/clase"
                android:layout_width="183dp"
                android:layout_height="30dp"
                android:gravity="center"
                android:text="CLASE"
